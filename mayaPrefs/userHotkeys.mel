hotkey -keyShortcut "6" -alt -name ("cupSelectSet6NameCommand");
hotkey -keyShortcut "r" -alt -name ("");
hotkey -keyShortcut "3" -alt -name ("cupSelectSet3NameCommand");
hotkey -keyShortcut "8" -alt -name ("cupSelectSet8NameCommand");
hotkey -keyShortcut "s" -ctl -alt -name ("incrementalSaveNameCommand");
