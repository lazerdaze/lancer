//
// The hotkey set
//
hotkeySet -source "Maya_Default" -current Maya_Default_Duplicate2;
//
// The hotkey contexts and hotkeys
//
hotkey -keyShortcut "2" -alt -name ("cupSelectSet2NameCommand");
hotkey -keyShortcut "7" -alt -name ("cupSelectSet7NameCommand");
hotkey -keyShortcut "s" -alt -name ("SetKeyframeWithColorNameCommand");
